                </engine:VisualElement>
                <engine:VisualElement style="flex-grow: 1; align-items: center; justify-content: flex-end; flex-direction: row;">
                    <engine:VisualElement name="coin-border" style="height: 80%; width: 35%; background-color: rgb(255, 255, 255); flex-direction: row; margin-right: 20px;">
                        <engine:VisualElement name="icon" style="background-image: url(&quot;project://database/Assets/02.Sprite/UI/Computer/Desktop/shop_icon.png?fileID=21300000&amp;guid=8b1ba4188779ade4f8527dca58c7fc2f&amp;type=3#shop_icon&quot;); width: 80px; height: 80px;" />

            <engine:VisualElement style="flex-grow: 1; flex-direction: row;">
                <engine:VisualElement name="left-contant" style="background-color: rgb(255, 229, 252); width: 60%;">
                    <engine:ScrollView name="category-scrollView" horizontal-scroller-visibility="Hidden" vertical-scroller-visibility="Hidden">
                        <engine:Instance template="ProductProfile" />
                    </engine:ScrollView>
                </engine:VisualElement>
                <engine:VisualElement name="right-contant" style="flex-grow: 1; background-color: rgb(229, 248, 255); padding-top: 30px; padding-left: 30px; padding-bottom: 30px; padding-right: 30px;">

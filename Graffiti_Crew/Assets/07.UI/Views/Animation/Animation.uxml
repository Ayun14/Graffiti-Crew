        <engine:VisualElement name="countdown1" class="countdown" style="background-image: url(&quot;project://database/Assets/02.Sprite/UI/Fight/Text/Countdown1.png?fileID=2800000&amp;guid=418a3457a178ca146ad5bf0b64592c9f&amp;type=3#Countdown1&quot;); height: 250px; width: 250px;" />
        <engine:VisualElement name="countdown2" class="countdown" style="background-image: url(&quot;project://database/Assets/02.Sprite/UI/Fight/Text/Countdown2.png?fileID=2800000&amp;guid=6499847f0900c764ebe786a98894ddae&amp;type=3#Countdown2&quot;); width: 250px; height: 250px;" />
        <engine:VisualElement name="countdown3" class="countdown" style="background-image: url(&quot;project://database/Assets/02.Sprite/UI/Fight/Text/Countdown3.png?fileID=2800000&amp;guid=ac44a9865f33b2942811773644c09dee&amp;type=3#Countdown3&quot;); width: 250px; height: 250px;" />
    </engine:VisualElement>
    <engine:VisualElement name="endAnimation" style="position: absolute; width: 100%; height: 100%; display: none;">
        <engine:VisualElement name="screen-content" style="flex-grow: 1; flex-direction: row;">
            <engine:VisualElement style="flex-grow: 1; flex-direction: row;">

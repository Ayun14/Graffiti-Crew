    <engine:VisualElement name="startAnimation" style="position: absolute; width: 100%; height: 100%; display: none;">
        <engine:VisualElement name="screen-content" style="flex-grow: 1; flex-direction: row;">
            <engine:VisualElement name="rival-screen" class="rival-screen">
                <engine:VisualElement name="VisualElement" class="blur" style="rotate: 180deg; background-image: url(&quot;project://database/Assets/02.Sprite/UI/Blur/fightAnimation-blur.png?fileID=2800000&amp;guid=20944e5283fe7cd40905ff703d73b36e&amp;type=3#fightAnimation-blur&quot;);" />
            </engine:VisualElement>

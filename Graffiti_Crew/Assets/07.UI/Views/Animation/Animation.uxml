            </engine:VisualElement>
            <engine:VisualElement name="vs" style="flex-grow: 1; position: absolute; background-image: url(&quot;project://database/Assets/02.Sprite/UI/Fight/Text/VS.png?fileID=21300000&amp;guid=2afaf2643e63e84488175e2c2fb3086c&amp;type=3#VS&quot;); width: 450px; height: 450px; top: 30%; left: 37%;" />
